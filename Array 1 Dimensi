#include <iostream>
using namespace std;

int main()
{
  int jumlah;
  int nilai [5];
  for (int i=0; i<5; i++)
  {
    cout << "Input Nilai ke-" <<(i+1)<<":";
    cin >> nilai[i];
  }
    jumlah = 0;
    for (int i=0; i<5; i++)
 {
 jumlah= jumlah + nilai[i];
 cout<<"Hasil ke-:" << i <<"adalah: " <<jumlah<<endl;
 }
    cout <<"\nJumlah Seluruhnya: "<<jumlah;
 return 0;
 }
